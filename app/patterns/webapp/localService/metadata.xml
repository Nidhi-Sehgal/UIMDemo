<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Alias="UI" Namespace="com.sap.vocabularies.UI.v1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="patterns" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <EntityContainer Name="EntityContainer">
                <EntitySet Name="patterns" EntityType="patterns.patterns">
                    <NavigationPropertyBinding Path="MaskingPatternStrategy" Target="strategies"/>
                    <NavigationPropertyBinding Path="texts" Target="patterns_texts"/>
                    <NavigationPropertyBinding Path="localized" Target="patterns_texts"/>
                    <NavigationPropertyBinding Path="SiblingEntity" Target="patterns"/>
                </EntitySet>
                <EntitySet Name="strategies" EntityType="patterns.strategies"/>
                <EntitySet Name="patterns_texts" EntityType="patterns.patterns_texts"/>
            </EntityContainer>
            <EntityType Name="patterns">
                <Key>
                    <PropertyRef Name="MaskingPatternName"/>
                    <PropertyRef Name="IsActiveEntity"/>
                </Key>
                <Property Name="MaskingPatternName" Type="Edm.String" MaxLength="10" Nullable="false"/>
                <NavigationProperty Name="MaskingPatternStrategy" Type="patterns.strategies">
                    <ReferentialConstraint Property="MaskingPatternStrategy_ID" ReferencedProperty="ID"/>
                </NavigationProperty>
                <Property Name="MaskingPatternStrategy_ID" Type="Edm.String" MaxLength="4"/>
                <Property Name="Set1MaskingStartPositionVal" Type="Edm.Int32"/>
                <Property Name="Set1MaskingCharacters" Type="Edm.String" MaxLength="40"/>
                <Property Name="Set2MaskingStartPositionVal" Type="Edm.Int32"/>
                <Property Name="Set2MaskingCharacters" Type="Edm.String" MaxLength="40"/>
                <Property Name="Set3MaskingStartPositionVal" Type="Edm.Int32"/>
                <Property Name="Set3MaskingCharacters" Type="Edm.String" MaxLength="40"/>
                <Property Name="EmptyFieldIsMasked" Type="Edm.Boolean" DefaultValue="true"/>
                <Property Name="MaskingPatternDescription" Type="Edm.String" MaxLength="128"/>
                <Property Name="HideSet" Type="Edm.Boolean"/>
                <Property Name="HideBadi" Type="Edm.Boolean"/>
                <NavigationProperty Name="texts" Type="Collection(patterns.patterns_texts)">
                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
                <NavigationProperty Name="localized" Type="patterns.patterns_texts">
                    <ReferentialConstraint Property="MaskingPatternName" ReferencedProperty="MaskingPatternName"/>
                </NavigationProperty>
                <Property Name="IsActiveEntity" Type="Edm.Boolean" Nullable="false" DefaultValue="true"/>
                <Property Name="HasActiveEntity" Type="Edm.Boolean" Nullable="false" DefaultValue="false"/>
                <Property Name="HasDraftEntity" Type="Edm.Boolean" Nullable="false" DefaultValue="false"/>
                <NavigationProperty Name="DraftAdministrativeData" Type="patterns.DraftAdministrativeData" ContainsTarget="true"/>
                <NavigationProperty Name="SiblingEntity" Type="patterns.patterns"/>
            </EntityType>
            <EntityType Name="strategies">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.String" MaxLength="4" Nullable="false"/>
                <Property Name="name" Type="Edm.String" MaxLength="40"/>
            </EntityType>
            <EntityType Name="DraftAdministrativeData">
                <Key>
                    <PropertyRef Name="DraftUUID"/>
                </Key>
                <Property Name="DraftUUID" Type="Edm.Guid" Nullable="false"/>
                <Property Name="CreationDateTime" Type="Edm.DateTimeOffset" Precision="7"/>
                <Property Name="CreatedByUser" Type="Edm.String" MaxLength="256"/>
                <Property Name="DraftIsCreatedByMe" Type="Edm.Boolean"/>
                <Property Name="LastChangeDateTime" Type="Edm.DateTimeOffset" Precision="7"/>
                <Property Name="LastChangedByUser" Type="Edm.String" MaxLength="256"/>
                <Property Name="InProcessByUser" Type="Edm.String" MaxLength="256"/>
                <Property Name="DraftIsProcessedByMe" Type="Edm.Boolean"/>
            </EntityType>
            <EntityType Name="patterns_texts">
                <Key>
                    <PropertyRef Name="locale"/>
                    <PropertyRef Name="MaskingPatternName"/>
                </Key>
                <Property Name="locale" Type="Edm.String" MaxLength="14" Nullable="false"/>
                <Property Name="MaskingPatternName" Type="Edm.String" MaxLength="10" Nullable="false"/>
                <Property Name="MaskingPatternDescription" Type="Edm.String" MaxLength="128"/>
            </EntityType>
            <Action Name="draftPrepare" IsBound="true" EntitySetPath="in">
                <Parameter Name="in" Type="patterns.patterns"/>
                <Parameter Name="SideEffectsQualifier" Type="Edm.String"/>
                <ReturnType Type="patterns.patterns"/>
            </Action>
            <Action Name="draftActivate" IsBound="true" EntitySetPath="in">
                <Parameter Name="in" Type="patterns.patterns"/>
                <ReturnType Type="patterns.patterns"/>
            </Action>
            <Action Name="draftEdit" IsBound="true" EntitySetPath="in">
                <Parameter Name="in" Type="patterns.patterns"/>
                <Parameter Name="PreserveChanges" Type="Edm.Boolean"/>
                <ReturnType Type="patterns.patterns"/>
            </Action>
            <Annotations Target="patterns.EntityContainer/patterns">
                <Annotation Term="Common.DraftRoot">
                    <Record Type="Common.DraftRootType">
                        <PropertyValue Property="ActivationAction" String="patterns.draftActivate"/>
                        <PropertyValue Property="EditAction" String="patterns.draftEdit"/>
                        <PropertyValue Property="PreparationAction" String="patterns.draftPrepare"/>
                    </Record>
                </Annotation>
            </Annotations>
            <Annotations Target="patterns.patterns/HideSet">
                <Annotation Term="Core.Computed" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.patterns/HideBadi">
                <Annotation Term="Core.Computed" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.patterns/IsActiveEntity">
                <Annotation Term="UI.Hidden" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.patterns/HasActiveEntity">
                <Annotation Term="UI.Hidden" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.patterns/HasDraftEntity">
                <Annotation Term="UI.Hidden" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.patterns/DraftAdministrativeData">
                <Annotation Term="UI.Hidden" Bool="true"/>
            </Annotations>
            <Annotations Target="patterns.DraftAdministrativeData">
                <Annotation Term="Common.Label" String="{i18n>Draft_DraftAdministrativeData}"/>
                </Annotations>
                <Annotations Target="patterns.DraftAdministrativeData/DraftUUID">
                    <Annotation Term="UI.Hidden" Bool="true"/>
                    <Annotation Term="Common.Label" String="{i18n>Draft_DraftUUID}"/>
                    </Annotations>
                    <Annotations Target="patterns.DraftAdministrativeData/CreationDateTime">
                        <Annotation Term="Common.Label" String="{i18n>Draft_CreationDateTime}"/>
                        </Annotations>
                        <Annotations Target="patterns.DraftAdministrativeData/CreatedByUser">
                            <Annotation Term="Common.Label" String="{i18n>Draft_CreatedByUser}"/>
                            </Annotations>
                            <Annotations Target="patterns.DraftAdministrativeData/DraftIsCreatedByMe">
                                <Annotation Term="UI.Hidden" Bool="true"/>
                                <Annotation Term="Common.Label" String="{i18n>Draft_DraftIsCreatedByMe}"/>
                                </Annotations>
                                <Annotations Target="patterns.DraftAdministrativeData/LastChangeDateTime">
                                    <Annotation Term="Common.Label" String="{i18n>Draft_LastChangeDateTime}"/>
                                    </Annotations>
                                    <Annotations Target="patterns.DraftAdministrativeData/LastChangedByUser">
                                        <Annotation Term="Common.Label" String="{i18n>Draft_LastChangedByUser}"/>
                                        </Annotations>
                                        <Annotations Target="patterns.DraftAdministrativeData/InProcessByUser">
                                            <Annotation Term="Common.Label" String="{i18n>Draft_InProcessByUser}"/>
                                            </Annotations>
                                            <Annotations Target="patterns.DraftAdministrativeData/DraftIsProcessedByMe">
                                                <Annotation Term="UI.Hidden" Bool="true"/>
                                                <Annotation Term="Common.Label" String="{i18n>Draft_DraftIsProcessedByMe}"/>
                                                </Annotations>
                                            </Schema>
                                        </edmx:DataServices>
                                    </edmx:Edmx>